  <body>
    <h1> My Todo List</h1>
    <div id="todo-table">
      <form>
        <input type="text" name="item" placeholder="Add new comment..." required />
        <button type="submit"> Add Comment</button>
        Counter:
      <form>
      <ul>
        <!-For loop to dynamically loop through lists in checklist->
        <% for(var i=0;i<todos.length;i++) { %>
          <li>(<%= todos[i].count %>) <%= todos[i].item %></li>
        <% } %>


       
      </ul>
    </div>
  </body>

